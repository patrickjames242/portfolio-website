    "mailgun-js": "^0.22.0",
    "observable-hooks": "^4.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-extend-components": "^1.2.12",
    "react-router-dom": "^6.19.0",
    "react-spring": "^9.7.3",
